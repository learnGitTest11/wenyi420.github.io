,CAACC,QAAb;AACD,KAFM,CAAP;AAGD,GARY;AASbA,EAAAA,QATa,oBASJH,KATI,EASG;AACd,WAAOA,KAAK,CAACF,MAAN,CAAa,UAACI,IAAD,EAAU;AAC5B,aAAOA,IAAI,CAACC,QAAZ;AACD,KAFM,CAAP;AAGD;AAbY,CAAf;AAgBA,eAAe,IAAIjB,IAAI,CAACkB,KAAT,CAAe;AAC5BC,EAAAA,MAAM,EAAE,IADoB;AAE5BC,EAAAA,KAAK,EAAE;AACLN,IAAAA,KAAK,EAAE,CACL;AACEO,MAAAA,OAAO,EAAE,cADX;AAEEJ,MAAAA,QAAQ,EAAE;AAFZ,KADK,EAKL;AACEI,MAAAA,OAAO,EAAE,cADX;AAEEJ,MAAAA,QAAQ,EAAE;AAFZ,KALK,EASL;AACEI,MAAAA,OAAO,EAAE,cADX;AAEEJ,MAAAA,QAAQ,EAAE;AAFZ,KATK;AADF,GAFqB;AAkB5BK,EAAAA,OAAO,EAAE;AACP;AACAC,IAAAA,SAFO,qBAEGH,KAFH,EAEU;AACf;AACA;AACA,aAAOR,MAAM,CAACQ,KAAK,CAACI,KAAN,CAAYC,IAAb,CAAN,CAAyBL,KAAK,CAACN,KAA/B,EAAsCY,GAAtC,CAA0C,UAACV,IAAD;AAAA,eAC/CI,KAAK,CAACN,KAAN,CAAYa,OAAZ,CAAoBX,IAApB,CAD+C;AAAA,OAA1C,CAAP;AAGD;AARM,GAlBmB;AA4B5BY,EAAAA,SAAS,EAAE;AACTC,IAAAA,SADS,qBACCT,KADD,EACQX,IADR,EACc;AACrBW,MAAAA,KAAK,CAACN,KAAN,GAAcL,IAAd,CADqB,CAErB;;AACAP,MAAAA,EAAE,CAACM,IAAH,CAAQY,KAAK,CAACN,KAAd;AACD,KALQ;AAMTgB,IAAAA,QANS,oBAMAV,KANA,EAMOX,IANP,EAMa;AACpBW,MAAAA,KAAK,CAACN,KAAN,CAAYiB,IAAZ,CAAiBtB,IAAjB,EADoB,CAEpB;;AACAP,MAAAA,EAAE,CAACM,IAAH,CAAQY,KAAK,CAACN,KAAd;AACD,KAVQ;AAWTkB,IAAAA,WAXS,uBAWGZ,KAXH,QAW2B;AAAA,UAAfa,KAAe,QAAfA,KAAe;AAAA,UAARxB,IAAQ,QAARA,IAAQ;AAClCW,MAAAA,KAAK,CAACN,KAAN,CAAYmB,KAAZ,IAAqBxB,IAArB;AACAP,MAAAA,EAAE,CAACM,IAAH,CAAQY,KAAK,CAACN,KAAd;AACD,KAdQ;AAeToB,IAAAA,WAfS,uBAeGd,KAfH,EAeUa,KAfV,EAeiB;AACxBb,MAAAA,KAAK,CAACN,KAAN,CAAYqB,MAAZ,CAAmBF,KAAnB,EAA0B,CAA1B;AACA/B,MAAAA,EAAE,CAACM,IAAH,CAAQY,KAAK,CAACN,KAAd;AACD;AAlBQ,GA5BiB;AAgD5BsB,EAAAA,OAAO,EAAE;AACP;AACAC,IAAAA,UAFO,6BAEgB;AAAA,UAAVC,MAAU,SAAVA,MAAU;AACrB;AACAA,MAAAA,MAAM,CAAC,WAAD,EAAcpC,EAAE,CAACC,IAAH,EAAd,CAAN;AACD;AALM,GAhDmB;AAuD5BoC,EAAAA,OAAO,EAAE;AAvDmB,CAAf,CAAf","sourcesContent":["import Vue from \"vue\";\nimport Vuex from \"vuex\";\n\nVue.use(Vuex);\n\n// localStorage 存取工具\nconst LS = {\n  load() {\n    return JSON.parse(localStorage.getItem(\"vue-todo\") || \"[]\");\n  },\n  save(data) {\n    localStorage.setItem(\"vue-todo\", JSON.stringify(data));\n  },\n};\n\n// 篩選資料\nconst filter = {\n  all(todos) {\n    return todos;\n  },\n  active(todos) {\n    return todos.filter((todo) => {\n      return !todo.complete;\n    });\n  },\n  complete(todos) {\n    return todos.filter((todo) => {\n      return todo.complete;\n    });\n  },\n};\n\nexport default new Vuex.Store({\n  strict: true,\n  state: {\n    todos: [\n      {\n        content: \"todo-content\",\n        complete: false,\n      },\n      {\n        content: \"todo-content\",\n        complete: true,\n      },\n      {\n        content: \"todo-content\",\n        complete: false,\n      },\n    ],\n  },\n  getters: {\n    //處理 index\n    todoIndex(state) {\n      // [state.route.name] 表示三種狀態 呼叫 filter 的 all() active() complete()\n      // 取得資料後再透過 map 處理  只需要他的 index 此為 state.todo的\n      return filter[state.route.name](state.todos).map((todo) =>\n        state.todos.indexOf(todo)\n      );\n    },\n  },\n  mutations: {\n    SET_TODOS(state, data) {\n      state.todos = data;\n      // save LS\n      LS.save(state.todos);\n    },\n    ADD_TODO(state, data) {\n      state.todos.push(data);\n      // 寫入 localStorage\n      LS.save(state.todos);\n    },\n    UPDATE_TODO(state, { index, data }) {\n      state.todos[index] = data;\n      LS.save(state.todos);\n    },\n    REMOVE_TODO(state, index) {\n      state.todos.splice(index, 1);\n      LS.save(state.todos);\n    },\n  },\n  actions: {\n    // 一開始從 localstorage 撈資料\n    INIT_TODOS({ commit }) {\n      // load LS\n      commit(\"SET_TODOS\", LS.load());\n    },\n  },\n  modules: {},\n});\n"]}]}